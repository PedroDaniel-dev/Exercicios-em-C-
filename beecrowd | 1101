#include <stdio.h>

int main() {
    int m, n, maior, menor;
    scanf("%d %d", &m, &n);
    if (m - n > 0) {
        maior = m;
        menor = n;
    }else {
        maior = n;
        menor = m;
    }
    
    if (menor <= 0) {
        return 0;
    }

    int soma = 0;
    while(menor <= maior) {
        printf("%d ", menor);
        soma += menor;
        menor++;
    }
    printf("Sum=%d", soma);
}
    
